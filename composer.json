{
    "name": "patricksamson/laravel-moneybags",
    "description": ":package_description",
    "keywords": [
        "patricksamson",
        "laravel",
        "laravel-moneybags",
        "money"
    ],
    "homepage": "https://github.com/patricksamson/laravel-moneybags",
    "license": "MIT",
    "authors": [
        {
            "name": ":Patrick Samson",
            "email": "patricksamson236@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.73"
    },
    "require-dev": {
        "fakerphp/faker": "^1.17",
        "orchestra/testbench": "^6.22",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "PatrickSamson\\LaravelMoneybags\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PatrickSamson\\LaravelMoneybags\\Tests\\": "tests"
        }
    },
    "scripts": {
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": false
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PatrickSamson\\LaravelMoneybags\\LaravelMoneybagsServiceProvider"
            ],
            "aliases": {
                "LaravelMoneybags": "PatrickSamson\\LaravelMoneybags\\Facades\\LaravelMoneybags"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
